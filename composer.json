{
  "name": "sniccowp/sniccowp",
  "description": "The SniccoWP framework",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    },
    {
      "name": "Marlon Alkan",
      "email": "marlon@snicco.de"
    }
  ],
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "ext-ctype": "*",
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "illuminate/collections": "^8.81",
    "illuminate/container": "^8.81",
    "illuminate/contracts": "^8.81",
    "illuminate/database": "^8.81",
    "illuminate/events": "^8.81",
    "illuminate/filesystem": "^8.81",
    "illuminate/macroable": "^8.81",
    "illuminate/support": "^8.81",
    "illuminate/view": "^8.81",
    "middlewares/negotiation": "^2.1.0",
    "nikic/fast-route": "1.3.0",
    "paragonie/constant_time_encoding": "^2.4",
    "pimple/pimple": "^3.4",
    "psr/container": "^1.1",
    "psr/event-dispatcher": "^1.0",
    "psr/http-factory": "^1.0.0",
    "psr/http-message": "^1.0.0",
    "psr/http-server-handler": "^1.0.0",
    "psr/http-server-middleware": "^1.0.0",
    "psr/log": "^1.1.1",
    "psr/simple-cache": "^1.0.0",
    "symfony/finder": "^5.0",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "cache/array-adapter": "^1.0.0",
    "cache/integration-tests": "0.17.0",
    "codeception/codeception": "4.1.29",
    "fakerphp/faker": "^1.0.0-stable",
    "lucatume/wp-browser": "~3.1.3",
    "nyholm/psr7": "^1.0.0",
    "php-stubs/wordpress-stubs": "^5.8.0",
    "phpunit/phpunit": "9.5.13",
    "symplify/monorepo-builder": "9.4.70",
    "vimeo/psalm": "^4.10",
    "vlucas/phpdotenv": "5.4.1"
  },
  "autoload": {
    "psr-4": {
      "Snicco\\Bridge\\Blade\\": "src/Snicco/Bridge/blade/src",
      "Snicco\\Bridge\\IlluminateContainer\\": "src/Snicco/Bridge/illuminate-container/src",
      "Snicco\\Bridge\\Pimple\\": "src/Snicco/Bridge/pimple/src",
      "Snicco\\Bridge\\SessionPsr16\\": "src/Snicco/Bridge/session-psr16/src",
      "Snicco\\Bridge\\SignedUrlMiddleware\\": "src/Snicco/Bridge/signed-url-middleware/src",
      "Snicco\\Bridge\\SignedUrlPsr16\\": "src/Snicco/Bridge/signed-url-psr16/src",
      "Snicco\\Component\\BetterWPHooks\\": "src/Snicco/Component/better-wp-hooks/src",
      "Snicco\\Component\\BetterWPMail\\": "src/Snicco/Component/better-wp-mail/src",
      "Snicco\\Component\\Eloquent\\": "src/Snicco/Component/eloquent/src",
      "Snicco\\Component\\EventDispatcher\\": "src/Snicco/Component/event-dispatcher/src",
      "Snicco\\Component\\HttpRouting\\": "src/Snicco/Component/http-routing/src",
      "Snicco\\Component\\Kernel\\": "src/Snicco/Component/kernel/src",
      "Snicco\\Component\\ParameterBag\\": "src/Snicco/Component/parameter-bag/src",
      "Snicco\\Component\\Psr7ErrorHandler\\": "src/Snicco/Component/psr7-error-handler/src",
      "Snicco\\Component\\ScopableWP\\": "src/Snicco/Component/scopable-wp/src",
      "Snicco\\Component\\Session\\": "src/Snicco/Component/session/src",
      "Snicco\\Component\\SignedUrl\\": "src/Snicco/Component/signed-url/src",
      "Snicco\\Component\\StrArr\\": "src/Snicco/Component/str-arr/src",
      "Snicco\\Component\\Templating\\": "src/Snicco/Component/templating/src",
      "Snicco\\Component\\TestableClock\\": "src/Snicco/Component/testable-clock/src",
      "Snicco\\Component\\WPObjectCachePsr16\\": "src/Snicco/Component/wp-object-cache-psr16/src",
      "Snicco\\Middleware\\DefaultHeaders\\": "src/Snicco/Middleware/default-headers/src",
      "Snicco\\Middleware\\GuestsOnly\\": "src/Snicco/Middleware/wp-guests-only/src",
      "Snicco\\Middleware\\HttpsOnly\\": "src/Snicco/Middleware/https-only/src",
      "Snicco\\Middleware\\MustMatchRoute\\": "src/Snicco/Middleware/must-match-route/src",
      "Snicco\\Middleware\\NoRobots\\": "src/Snicco/Middleware/no-robots/src",
      "Snicco\\Middleware\\OpenRedirectProtection\\": "src/Snicco/Middleware/open-redirect-protection/src",
      "Snicco\\Middleware\\Payload\\": "src/Snicco/Middleware/payload/src",
      "Snicco\\Middleware\\Redirect\\": "src/Snicco/Middleware/redirect/src",
      "Snicco\\Middleware\\ShareCookies\\": "src/Snicco/Middleware/share-cookies/src",
      "Snicco\\Middleware\\TrailingSlash\\": "src/Snicco/Middleware/trailing-slash/src",
      "Snicco\\Middleware\\WPAuth\\": "src/Snicco/Middleware/wp-auth-only/src",
      "Snicco\\Middleware\\WPCap\\": "src/Snicco/Middleware/wp-capability/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Snicco\\Bridge\\Blade\\Tests\\": "src/Snicco/Bridge/blade/tests",
      "Snicco\\Bridge\\IlluminateContainer\\Tests\\": "src/Snicco/Bridge/illuminate-container/tests",
      "Snicco\\Bridge\\Pimple\\Tests\\": "src/Snicco/Bridge/pimple/tests",
      "Snicco\\Bridge\\SessionPsr16\\Tests\\": "src/Snicco/Bridge/session-psr16/tests",
      "Snicco\\Bridge\\SignedUrlMiddleware\\Tests\\": "src/Snicco/Bridge/signed-url-middleware/tests",
      "Snicco\\Bridge\\SignedUrlPsr16\\Tests\\": "src/Snicco/Bridge/signed-url-psr16/tests",
      "Snicco\\Component\\BetterWPHooks\\Tests\\": "src/Snicco/Component/better-wp-hooks/tests",
      "Snicco\\Component\\BetterWPMail\\Testing\\": "src/Snicco/Component/better-wp-mail/testing",
      "Snicco\\Component\\BetterWPMail\\Tests\\": "src/Snicco/Component/better-wp-mail/tests",
      "Snicco\\Component\\Eloquent\\Tests\\": "src/Snicco/Component/eloquent/tests",
      "Snicco\\Component\\EventDispatcher\\Tests\\": "src/Snicco/Component/event-dispatcher/tests",
      "Snicco\\Component\\HttpRouting\\Testing\\": "src/Snicco/Component/http-routing/testing",
      "Snicco\\Component\\HttpRouting\\Tests\\": "src/Snicco/Component/http-routing/tests",
      "Snicco\\Component\\Kernel\\Testing\\": "src/Snicco/Component/kernel/testing",
      "Snicco\\Component\\Kernel\\Tests\\": "src/Snicco/Component/kernel/tests",
      "Snicco\\Component\\ParameterBag\\Tests\\": "src/Snicco/Component/parameter-bag/tests",
      "Snicco\\Component\\Psr7ErrorHandler\\Tests\\": "src/Snicco/Component/psr7-error-handler/tests",
      "Snicco\\Component\\ScopableWP\\Tests\\": "src/Snicco/Component/scopable-wp/tests",
      "Snicco\\Component\\Session\\Testing\\": "src/Snicco/Component/session/testing",
      "Snicco\\Component\\Session\\Tests\\": "src/Snicco/Component/session/tests",
      "Snicco\\Component\\SignedUrl\\Testing\\": "src/Snicco/Component/signed-url/testing",
      "Snicco\\Component\\SignedUrl\\Tests\\": "src/Snicco/Component/signed-url/tests",
      "Snicco\\Component\\StrArr\\Tests\\": "src/Snicco/Component/str-arr/tests",
      "Snicco\\Component\\Templating\\Tests\\": "src/Snicco/Component/templating/tests",
      "Snicco\\Component\\TestableClock\\Tests\\": "src/Snicco/Component/testable-clock/tests",
      "Snicco\\Component\\WPObjectCachePsr16\\Tests\\": "src/Snicco/Component/wp-object-cache-psr16/tests",
      "Snicco\\Middleware\\DefaultHeaders\\Tests\\": "src/Snicco/Middleware/default-headers/tests",
      "Snicco\\Middleware\\GuestsOnly\\Tests\\": "src/Snicco/Middleware/wp-guests-only/tests",
      "Snicco\\Middleware\\HttpsOnly\\Tests\\": "src/Snicco/Middleware/https-only/tests",
      "Snicco\\Middleware\\MustMatchRoute\\Tests\\": "src/Snicco/Middleware/must-match-route/tests",
      "Snicco\\Middleware\\NoRobots\\Tests\\": "src/Snicco/Middleware/no-robots/tests",
      "Snicco\\Middleware\\OpenRedirectProtection\\Tests\\": "src/Snicco/Middleware/open-redirect-protection/tests",
      "Snicco\\Middleware\\Payload\\Tests\\": "src/Snicco/Middleware/payload/tests",
      "Snicco\\Middleware\\Redirect\\Tests\\": "src/Snicco/Middleware/redirect/tests",
      "Snicco\\Middleware\\ShareCookies\\Tests\\": "src/Snicco/Middleware/share-cookies/tests",
      "Snicco\\Middleware\\TrailingSlash\\Tests\\": "src/Snicco/Middleware/trailing-slash/tests",
      "Snicco\\Middleware\\WPAuth\\Tests\\": "src/Snicco/Middleware/wp-auth-only/tests",
      "Snicco\\Middleware\\WPCap\\Tests\\": "src/Snicco/Middleware/wp-capability/tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "phpunit:all": "vendor/bin/phpunit --testsuite unit",
    "phpunit:component": "vendor/bin/phpunit --testsuite component",
    "phpunit:middleware": "vendor/bin/phpunit --testsuite middleware",
    "phpunit:bridge": "vendor/bin/phpunit --testsuite bridge",
    "codeception:all": "vendor/bin/codecept run -c codeception/all.dist.yml",
    "codeception:component": "vendor/bin/codecept run -c codeception/component.dist.yml",
    "codeception:bridge": "vendor/bin/codecept run -c codeception/bridge.dist.yml",
    "tests": [
      "@phpunit:all",
      "@codeception:all"
    ],
    "tests:component": [
      "@phpunit:component",
      "@codeception:component"
    ],
    "tests:middleware": [
      "@phpunit:middleware"
    ],
    "tests:bridge": [
      "@phpunit:bridge",
      "@codeception:bridge"
    ],
    "merge": [
      "vendor/bin/monorepo-builder merge",
      "composer dump-autoload"
    ],
    "propagate": [
      "vendor/bin/monorepo-builder propagate"
    ],
    "deptrac": "/usr/local/bin/deptrac --config-file=deptrac/deptrac.yaml",
    "psalm": "vendor/bin/psalm",
    "psalm:no-baseline": "vendor/bin/psalm --ignore-baseline",
    "psalm:clear-cache": [
      "vendor/bin/psalm --clear-cache",
      "vendor/bin/psalm --clear-global-cache"
    ],
    "psalm:update-baseline": [
      "@psalm:clear-cache",
      "@psalm --update-baseline"
    ]
  },
  "replace": {
    "sniccowp/better-wp-hooks": "self.version",
    "sniccowp/better-wp-mail": "self.version",
    "sniccowp/blade-bridge": "self.version",
    "sniccowp/default-headers-middleware": "self.version",
    "sniccowp/eloquent": "self.version",
    "sniccowp/event-dispatcher": "self.version",
    "sniccowp/guests-only-middleware": "self.version",
    "sniccowp/http-routing": "self.version",
    "sniccowp/https-only-middleware": "self.version",
    "sniccowp/illuminate-container-brige": "self.version",
    "sniccowp/kernel": "self.version",
    "sniccowp/must-match-route-middleware": "self.version",
    "sniccowp/no-robots-middleware": "self.version",
    "sniccowp/open-redirect-protection-middleware": "self.version",
    "sniccowp/paramter-bag": "self.version",
    "sniccowp/payload-middleware": "self.version",
    "sniccowp/pimple-brige": "self.version",
    "sniccowp/psr-7-error-handler": "self.version",
    "sniccowp/redirect-middleware": "self.version",
    "sniccowp/scopable-wp": "self.version",
    "sniccowp/session": "self.version",
    "sniccowp/session-psr16": "self.version",
    "sniccowp/share-cookies-middleware": "self.version",
    "sniccowp/signed-url": "self.version",
    "sniccowp/signed-url-middleware": "self.version",
    "sniccowp/signed-url-psr16": "self.version",
    "sniccowp/str-arr": "self.version",
    "sniccowp/templating": "self.version",
    "sniccowp/testable-clock": "self.version",
    "sniccowp/trailing-slash-middleware": "self.version",
    "sniccowp/wp-auth-only-middleware": "self.version",
    "sniccowp/wp-capapility-middleware": "self.version",
    "sniccowp/wp-object-cache-psr16": "self.version"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
