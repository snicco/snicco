{
    "name": "snicco/event-dispatcher-testing",
    "license": "LGPL-3.0-only",
    "description": "Provides a testable adapter for the snicco/event-dispatcher component",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "phpunit/phpunit": "^9.5.13",
        "snicco/event-dispatcher": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Component\\EventDispatcher\\Testing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Component\\EventDispatcher\\Tests\\Testing\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "conflict": {
        "snicco/http-routing": "<1.6.0",
