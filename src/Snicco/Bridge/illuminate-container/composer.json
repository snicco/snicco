{
    "name": "snicco/illuminate-container-bridge",
    "description": "Provides an adapter for the Laravel DI container for snicco/kernel",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/container": "^8.81",
        "illuminate/contracts": "^8.81",
        "snicco/kernel": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "snicco/kernel-testing": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Bridge\\IlluminateContainer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Bridge\\IlluminateContainer\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "conflict": {
        "snicco/http-routing": "<2.0.0-beta.2",
        "snicco/testable-clock": "<2.0.0-beta.2",
        "snicco/signed-url": "<2.0.0-beta.2",
        "snicco/templating": "<2.0.0-beta.2",
        "snicco/psr7-error-handler": "<2.0.0-beta.2",
        "snicco/better-wpdb": "<2.0.0-beta.2",
        "snicco/str-arr": "<2.0.0-beta.2",
        "snicco/better-wp-hooks": "<2.0.0-beta.2",
        "snicco/event-dispatcher": "<2.0.0-beta.2",
        "snicco/eloquent": "<2.0.0-beta.2",
        "snicco/better-wp-mail": "<2.0.0-beta.2",
        "snicco/better-wp-cache": "<2.0.0-beta.2",
        "snicco/session": "<2.0.0-beta.2",
        "snicco/better-wp-api": "<2.0.0-beta.2",
        "snicco/session-wp-bridge": "<2.0.0-beta.2",
        "snicco/session-psr16-bridge": "<2.0.0-beta.2",
        "snicco/blade-bridge": "<2.0.0-beta.2",
        "snicco/signed-url-psr16-bridge": "<2.0.0-beta.2",
        "snicco/signed-url-psr15-bridge": "<2.0.0-beta.2",
        "snicco/pimple-bridge": "<2.0.0-beta.2",
        "snicco/no-robots-middleware": "<2.0.0-beta.2",
        "snicco/open-redirect-protection-middleware": "<2.0.0-beta.2",
        "snicco/wp-capapility-middleware": "<1.0.0",
        "snicco/wp-nonce-middleware": "<2.0.0-beta.2",
        "snicco/payload-middleware": "<2.0.0-beta.2",
        "snicco/default-headers-middleware": "<2.0.0-beta.2",
        "snicco/wp-auth-only-middleware": "<2.0.0-beta.2",
        "snicco/content-negotiation-middleware": "<2.0.0-beta.2",
        "snicco/redirect-middleware": "<2.0.0-beta.2",
        "snicco/must-match-route-middleware": "<2.0.0-beta.2",
        "snicco/method-override-middleware": "<2.0.0-beta.2",
        "snicco/share-cookies-middleware": "<2.0.0-beta.2",
        "snicco/https-only-middleware": "<2.0.0-beta.2",
        "snicco/guests-only-middleware": "<1.0.0",
        "snicco/trailing-slash-middleware": "<2.0.0-beta.2",
        "snicco/testing-bundle": "<2.0.0-beta.2",
        "snicco/http-routing-bundle": "<2.0.0-beta.2",
        "snicco/debug-bundle": "<2.0.0-beta.2",
        "snicco/templating-bundle": "<2.0.0-beta.2",
        "snicco/better-wpdb-bundle": "<2.0.0-beta.2",
        "snicco/better-wp-hooks-bundle": "<2.0.0-beta.2",
        "snicco/blade-bundle": "<2.0.0-beta.2",
        "snicco/better-wp-mail-bundle": "<2.0.0-beta.2",
        "snicco/better-wp-cache-bundle": "<2.0.0-beta.2",
        "snicco/session-bundle": "<2.0.0-beta.2",
        "snicco/encryption-bundle": "<2.0.0-beta.2",
        "snicco/wp-guests-only-middleware": "<2.0.0-beta.2",
        "snicco/wp-capability-middleware": "<2.0.0-beta.2",
        "snicco/better-wp-mail-testing": "<2.0.0-beta.2",
        "snicco/session-testing": "<2.0.0-beta.2",
        "snicco/kernel-testing": "<2.0.0-beta.2",
        "snicco/signed-url-testing": "<2.0.0-beta.2",
        "snicco/http-routing-testing": "<2.0.0-beta.2",
        "snicco/event-dispatcher-testing": "<2.0.0-beta.2",
        "snicco/better-wp-cli": "<2.0.0-beta.2",
        "snicco/signed-url-wp-bridge": "<2.0.0-beta.2",
