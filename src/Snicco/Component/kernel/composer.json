{
    "name": "snicco/kernel",
    "description": "The core of the snicco framework",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "psr/container": "^1.1",
        "snicco/str-arr": "^1.9",
        "webimpress/safe-writer": "^2.2",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "snicco/pimple-bridge": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Component\\Kernel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Component\\Kernel\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "conflict": {
        "snicco/http-routing": "<1.9.0",
        "snicco/testable-clock": "<1.9.0",
        "snicco/signed-url": "<1.9.0",
        "snicco/templating": "<1.9.0",
        "snicco/psr7-error-handler": "<1.9.0",
        "snicco/better-wpdb": "<1.9.0",
        "snicco/better-wp-hooks": "<1.9.0",
        "snicco/event-dispatcher": "<1.9.0",
        "snicco/eloquent": "<1.9.0",
        "snicco/better-wp-mail": "<1.9.0",
        "snicco/better-wp-cache": "<1.9.0",
        "snicco/session": "<1.9.0",
        "snicco/better-wp-api": "<1.9.0",
        "snicco/session-wp-bridge": "<1.9.0",
        "snicco/session-psr16-bridge": "<1.9.0",
        "snicco/blade-bridge": "<1.9.0",
        "snicco/signed-url-psr16-bridge": "<1.9.0",
        "snicco/illuminate-container-bridge": "<1.9.0",
        "snicco/signed-url-psr15-bridge": "<1.9.0",
        "snicco/pimple-bridge": "<1.9.0",
        "snicco/no-robots-middleware": "<1.9.0",
        "snicco/open-redirect-protection-middleware": "<1.9.0",
        "snicco/wp-capapility-middleware": "<1.0.0",
        "snicco/wp-nonce-middleware": "<1.9.0",
        "snicco/payload-middleware": "<1.9.0",
        "snicco/default-headers-middleware": "<1.9.0",
        "snicco/wp-auth-only-middleware": "<1.9.0",
        "snicco/content-negotiation-middleware": "<1.9.0",
        "snicco/redirect-middleware": "<1.9.0",
        "snicco/must-match-route-middleware": "<1.9.0",
        "snicco/method-override-middleware": "<1.9.0",
        "snicco/share-cookies-middleware": "<1.9.0",
        "snicco/https-only-middleware": "<1.9.0",
        "snicco/guests-only-middleware": "<1.0.0",
        "snicco/trailing-slash-middleware": "<1.9.0",
        "snicco/testing-bundle": "<1.9.0",
        "snicco/http-routing-bundle": "<1.9.0",
        "snicco/debug-bundle": "<1.9.0",
        "snicco/templating-bundle": "<1.9.0",
        "snicco/better-wpdb-bundle": "<1.9.0",
        "snicco/better-wp-hooks-bundle": "<1.9.0",
        "snicco/blade-bundle": "<1.9.0",
        "snicco/better-wp-mail-bundle": "<1.9.0",
        "snicco/better-wp-cache-bundle": "<1.9.0",
        "snicco/session-bundle": "<1.9.0",
        "snicco/encryption-bundle": "<1.9.0",
        "snicco/wp-guests-only-middleware": "<1.9.0",
        "snicco/wp-capability-middleware": "<1.9.0",
        "snicco/better-wp-mail-testing": "<1.9.0",
        "snicco/session-testing": "<1.9.0",
        "snicco/kernel-testing": "<1.9.0",
        "snicco/signed-url-testing": "<1.9.0",
        "snicco/http-routing-testing": "<1.9.0",
        "snicco/event-dispatcher-testing": "<1.9.0",
        "snicco/better-wp-cli": "<1.9.0",
        "snicco/signed-url-wp-bridge": "<1.9.0",
        "snicco/minimal-logger": "<1.9.0",
        "snicco/better-wp-cli-testing": "<1.9.0"
    },
    "license": "LGPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true
}
