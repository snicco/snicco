        "sniccowp/https-only-middleware": "<1.4.0",
        "sniccowp/guests-only-middleware": "<1.4.0",
        "sniccowp/trailing-slash-middleware": "<1.4.0",
        "sniccowp/testing-bundle": "<1.4.0",
        "sniccowp/http-routing-bundle": "<1.4.0",
        "sniccowp/debug-bundle": "<1.4.0",
