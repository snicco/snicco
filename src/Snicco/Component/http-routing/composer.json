{
    "name": "sniccowp/http-routing",
    "description": "The HTTP and routing component of the sniccowp framework",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-filter": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "nikic/fast-route": "1.3.0",
        "psr/container": "^1.1",
        "psr/http-factory": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "psr/http-server-handler": "^1.0.0",
        "psr/http-server-middleware": "^1.0.0",
        "sniccowp/psr7-error-handler": "dev-master",
        "sniccowp/str-arr": "^1.4",
        "webimpress/safe-writer": "^2.2",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "nyholm/psr7": "^1.0.0",
        "pimple/pimple": "^3.5"
    },
    "suggest": {
        "nyholm/psr7": "To use as a fast PSR7 implementation."
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Component\\HttpRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Component\\HttpRouting\\Tests\\": "tests",
            "Snicco\\Component\\HttpRouting\\Testing\\": "testing"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "conflict": {
        "sniccowp/testable-clock": "<1.4.0",
        "sniccowp/signed-url": "<1.4.0",
        "sniccowp/templating": "<1.4.0",
        "sniccowp/psr-7-error-handler": "<1.4.0",
        "sniccowp/better-wpdb": "<1.4.0",
        "sniccowp/better-wp-hooks": "<1.4.0",
        "sniccowp/event-dispatcher": "<1.4.0",
        "sniccowp/eloquent": "<1.4.0",
        "sniccowp/better-wp-mail": "<1.4.0",
        "sniccowp/better-wp-cache": "<1.4.0",
        "sniccowp/session": "<1.4.0",
        "sniccowp/better-wp-api": "<1.4.0",
        "sniccowp/kernel": "<1.4.0",
        "sniccowp/session-wp": "<1.4.0",
        "sniccowp/session-psr16": "<1.4.0",
        "sniccowp/blade-bridge": "<1.4.0",
        "sniccowp/signed-url-psr16": "<1.4.0",
        "sniccowp/illuminate-container-brige": "<1.4.0",
        "sniccowp/signed-url-middleware": "<1.4.0",
        "sniccowp/pimple-brige": "<1.4.0",
        "sniccowp/no-robots-middleware": "<1.4.0",
        "sniccowp/open-redirect-protection-middleware": "<1.4.0",
        "sniccowp/wp-capapility-middleware": "<1.4.0",
        "sniccowp/wp-nonce-middleware": "<1.4.0",
        "sniccowp/payload-middleware": "<1.4.0",
        "sniccowp/default-headers-middleware": "<1.4.0",
        "sniccowp/wp-auth-only-middleware": "<1.4.0",
        "sniccowp/content-negotiation-middleware": "<1.4.0",
        "sniccowp/redirect-middleware": "<1.4.0",
        "sniccowp/must-match-route-middleware": "<1.4.0",
        "sniccowp/method-override-middleware": "<1.4.0",
        "sniccowp/share-cookies-middleware": "<1.4.0",
        "sniccowp/https-only-middleware": "<1.4.0",
        "sniccowp/guests-only-middleware": "<1.4.0",
        "sniccowp/trailing-slash-middleware": "<1.4.0",
        "sniccowp/testing-bundle": "<1.4.0",
        "sniccowp/http-routing-bundle": "<1.4.0",
        "sniccowp/debug-bundle": "<1.4.0",
        "sniccowp/templating-bundle": "<1.4.0",
        "sniccowp/better-wpdb-bundle": "<1.4.0",
        "sniccowp/better-wp-hooks-bundle": "<1.4.0",
        "sniccowp/blade-bundle": "<1.4.0",
        "sniccowp/better-wp-mail-bundle": "<1.4.0",
        "sniccowp/better-wp-cache-bundle": "<1.4.0",
        "sniccowp/session-bundle": "<1.4.0",
        "sniccowp/encryption-bundle": "<1.4.0"
    }
}
