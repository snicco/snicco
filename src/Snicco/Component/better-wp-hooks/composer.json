{
    "name": "sniccowp/better-wp-hooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Calvin Alkan",
            "email": "calvin@snicco.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "sniccowp/better-wp-api": "^1.0",
        "sniccowp/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "lucatume/wp-browser": "~3.1.4",
        "phpunit/phpunit": "9.5.13"
    },
    "autoload": {
        "psr-4": {
            "Snicco\\Component\\BetterWPHooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snicco\\Component\\BetterWPHooks\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "conflict": {
        "sniccowp/http-routing": "<1.0.0",
        "sniccowp/testable-clock": "<1.0.0",
        "sniccowp/signed-url": "<1.0.0",
        "sniccowp/templating": "<1.0.0",
        "sniccowp/psr7-error-handler": "<1.0.0",
        "sniccowp/better-wpdb": "<1.0.0",
        "sniccowp/str-arr": "<1.0.0",
        "sniccowp/eloquent": "<1.0.0",
        "sniccowp/better-wp-mail": "<1.0.0",
        "sniccowp/better-wp-cache": "<1.0.0",
        "sniccowp/session": "<1.0.0",
        "sniccowp/kernel": "<1.0.0",
        "sniccowp/session-wp-bridge": "<1.0.0",
        "sniccowp/session-psr16-bridge": "<1.0.0",
        "sniccowp/blade-bridge": "<1.0.0",
        "sniccowp/signed-url-psr16-bridge": "<1.0.0",
        "sniccowp/illuminate-container-bridge": "<1.0.0",
        "sniccowp/signed-url-psr15-bridge": "<1.0.0",
        "sniccowp/pimple-bridge": "<1.0.0",
        "sniccowp/no-robots-middleware": "<1.0.0",
        "sniccowp/open-redirect-protection-middleware": "<1.0.0",
        "sniccowp/wp-capapility-middleware": "<1.0.0",
        "sniccowp/wp-nonce-middleware": "<1.0.0",
