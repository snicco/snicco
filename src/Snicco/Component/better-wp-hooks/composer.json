        "sniccowp/default-headers-middleware": "<1.0.0",
        "sniccowp/wp-auth-only-middleware": "<1.0.0",
        "sniccowp/content-negotiation-middleware": "<1.0.0",
        "sniccowp/redirect-middleware": "<1.0.0",
        "sniccowp/must-match-route-middleware": "<1.0.0",
        "sniccowp/method-override-middleware": "<1.0.0",
        "sniccowp/share-cookies-middleware": "<1.0.0",
        "sniccowp/https-only-middleware": "<1.0.0",
        "sniccowp/guests-only-middleware": "<1.0.0",
        "sniccowp/trailing-slash-middleware": "<1.0.0",
        "sniccowp/testing-bundle": "<1.0.0",
        "sniccowp/http-routing-bundle": "<1.0.0",
        "sniccowp/debug-bundle": "<1.0.0",
        "sniccowp/templating-bundle": "<1.0.0",
        "sniccowp/better-wpdb-bundle": "<1.0.0",
        "sniccowp/better-wp-hooks-bundle": "<1.0.0",
        "sniccowp/blade-bundle": "<1.0.0",
        "sniccowp/better-wp-mail-bundle": "<1.0.0",
        "sniccowp/better-wp-cache-bundle": "<1.0.0",
